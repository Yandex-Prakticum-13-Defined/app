{
  "name": "arkanoid",
  "version": "1.0.0",
  "description": "arkanoid game",
  "scripts": {
    "build": "cross-env NODE_ENV=production rimraf dist && webpack --mode production",
    "start:webpack": "webpack --mode=development --watch",
    "start:server": "wait-on dist/server.js && nodemon dist/server.js --watch dist/server.js",
    "start": "cross-env NODE_ENV=development npm-run-all --print-label --parallel start:*",
    "lint": "eslint . --fix",
    "stylelint": "npx stylelint **/*.scss --fix",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yandex-Prakticum-13-Defined/arkanoid.git"
  },
  "author": "Sergey Podryabinnikov, Aleksandr Korostelev, Tatiana Malashkina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yandex-Prakticum-13-Defined/arkanoid/issues"
  },
  "homepage": "https://github.com/Yandex-Prakticum-13-Defined/arkanoid#readme",
  "dependencies": {
    "@reduxjs/toolkit": "1.8.1",
    "axios": "0.27.2",
    "compression": "1.7.4",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "framer-motion": "6.3.3",
    "pg": "8.7.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "7.30.0",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "sequelize": "6.20.1",
    "sequelize-typescript": "2.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.1.1",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.0",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/react-test-renderer": "18.0.0",
    "@types/webpack-node-externals": "2.5.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "autoprefixer": "10.4.7",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "cssnano": "5.1.7",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "4.0.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "lint-staged": "12.4.1",
    "mini-css-extract-plugin": "2.6.0",
    "nodemon": "2.0.16",
    "npm-run-all": "4.1.5",
    "null-loader": "4.0.1",
    "postcss-loader": "6.2.1",
    "react-test-renderer": "18.1.0",
    "rimraf": "3.0.2",
    "sass": "1.51.0",
