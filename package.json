    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.0",
    "webpack-node-externals": "3.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint",
    "*.scss": "npm run stylelint",
    "./src/components/**/*.tsx": "jest --findRelatedTests"
  }
}
